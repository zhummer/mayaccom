<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Accomodation entity" internalId="4e98dd72-5a4f-4f82-9cb7-ead3984ea9af"><inputs name="host_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_accomodations_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="host"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Host.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Accomodation related_accomodations = accomodationDAO.findAccomodationByPrimaryKey(related_accomodations_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;Host host = hostDAO.findHostByPrimaryKey(host_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;related_accomodations.setHost(null);&#xD;&#xA;host.getAccomodations().remove(related_accomodations);&#x9;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;accomodationDAO.remove(related_accomodations);&#xD;&#xA;&#x9;&#x9;accomodationDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#xD;&#xA;return host;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
