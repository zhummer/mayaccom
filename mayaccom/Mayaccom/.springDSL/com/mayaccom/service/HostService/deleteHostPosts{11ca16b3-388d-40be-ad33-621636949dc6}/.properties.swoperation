<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Post entity" internalId="11ca16b3-388d-40be-ad33-621636949dc6"><inputs name="host_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_posts_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="host"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Host.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Post related_posts = postDAO.findPostByPrimaryKey(related_posts_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;Host host = hostDAO.findHostByPrimaryKey(host_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;related_posts.setHost(null);&#xD;&#xA;host.getPosts().remove(related_posts);&#x9;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;postDAO.remove(related_posts);&#xD;&#xA;&#x9;&#x9;postDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#xD;&#xA;return host;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
