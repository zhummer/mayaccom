<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Host entity" internalId="1dce66d7-4d0b-4888-9b1b-f4535412619b"><inputs name="host"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Host.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="Host existingHost = hostDAO.findHostByPrimaryKey(host.getId());&#xD;&#xA;&#xD;&#xA;if (existingHost != null)&#x9;{&#xD;&#xA;&#x9;if (existingHost != host) {&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setId (host.getId());&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setContactPerson (host.getContactPerson());&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setPhone1 (host.getPhone1());&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setPhone2 (host.getPhone2());&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setPhone3 (host.getPhone3());&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setEmail (host.getEmail());&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setUsername (host.getUsername());&#xD;&#xA;&#x9;&#x9;&#x9;existingHost.setPassword (host.getPassword());&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;host = hostDAO.store(existingHost);&#xD;&#xA;}&#xD;&#xA;else&#x9;{&#xD;&#xA;&#x9;host = hostDAO.store(host);&#xD;&#xA;}&#xD;&#xA;hostDAO.flush();&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
