<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Authorities entity" internalId="1387ba11-a4dd-4b0b-a850-06dd3c3fdf09"><inputs name="users_id"><dataType xsi:type="type:PrimitiveDataType"/></inputs><inputs name="related_authoritieses_id"><dataType xsi:type="type:PrimitiveDataType"/></inputs><outputs name="users"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Users.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Users users = usersDAO.findUsersByPrimaryKey(users_id, -1, -1);&#xD;&#xA;&#x9;&#x9;Authorities related_authoritieses = authoritiesDAO.findAuthoritiesByPrimaryKey(related_authoritieses_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;users.getAuthoritieses().remove(related_authoritieses);&#xD;&#xA;related_authoritieses.getUserses().remove(users);&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;users = usersDAO.store(users);&#xD;&#xA;&#x9;&#x9;usersDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;related_authoritieses = authoritiesDAO.store(related_authoritieses);&#xD;&#xA;&#x9;&#x9;authoritiesDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;authoritiesDAO.remove(related_authoritieses);&#xD;&#xA;&#x9;&#x9;authoritiesDAO.flush();&#xD;&#xA;&#xD;&#xA;return users;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
