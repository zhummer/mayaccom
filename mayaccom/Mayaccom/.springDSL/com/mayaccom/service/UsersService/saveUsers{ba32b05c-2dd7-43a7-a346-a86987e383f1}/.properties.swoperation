<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Users entity" internalId="ba32b05c-2dd7-43a7-a346-a86987e383f1"><inputs name="users"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Users.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="Users existingUsers = usersDAO.findUsersByPrimaryKey(users.getId());&#xD;&#xA;&#xD;&#xA;if (existingUsers != null)&#x9;{&#xD;&#xA;&#x9;if (existingUsers != users) {&#xD;&#xA;&#x9;&#x9;&#x9;existingUsers.setId (users.getId());&#xD;&#xA;&#x9;&#x9;&#x9;existingUsers.setEnabled (users.getEnabled());&#xD;&#xA;&#x9;&#x9;&#x9;existingUsers.setPassword (users.getPassword());&#xD;&#xA;&#x9;&#x9;&#x9;existingUsers.setUsername (users.getUsername());&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;users = usersDAO.store(existingUsers);&#xD;&#xA;}&#xD;&#xA;else&#x9;{&#xD;&#xA;&#x9;users = usersDAO.store(users);&#xD;&#xA;}&#xD;&#xA;usersDAO.flush();&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
