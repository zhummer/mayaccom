<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Host entity" internalId="f082a6fa-5ab6-46d9-b66b-57c76b2c5c93"><inputs name="post_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_host_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="post"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Post.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Post post = postDAO.findPostByPrimaryKey(post_id, -1, -1);&#xD;&#xA;&#x9;&#x9;Host related_host = hostDAO.findHostByPrimaryKey(related_host_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;post.setHost(null);&#xD;&#xA;related_host.getPosts().remove(post);&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;post = postDAO.store(post);&#xD;&#xA;&#x9;&#x9;postDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;related_host = hostDAO.store(related_host);&#xD;&#xA;&#x9;&#x9;hostDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;hostDAO.remove(related_host);&#xD;&#xA;&#x9;&#x9;hostDAO.flush();&#xD;&#xA;&#xD;&#xA;return post;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
