<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Blog entity" internalId="1bd38a07-dcd8-482e-ac0c-66c864429c99"><inputs name="blog"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Blog.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="Blog existingBlog = blogDAO.findBlogByPrimaryKey(blog.getId());&#xD;&#xA;&#xD;&#xA;if (existingBlog != null)&#x9;{&#xD;&#xA;&#x9;if (existingBlog != blog) {&#xD;&#xA;&#x9;&#x9;&#x9;existingBlog.setId (blog.getId());&#xD;&#xA;&#x9;&#x9;&#x9;existingBlog.setTitle (blog.getTitle());&#xD;&#xA;&#x9;&#x9;&#x9;existingBlog.setDescription (blog.getDescription());&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;blog = blogDAO.store(existingBlog);&#xD;&#xA;}&#xD;&#xA;else&#x9;{&#xD;&#xA;&#x9;blog = blogDAO.store(blog);&#xD;&#xA;}&#xD;&#xA;blogDAO.flush();&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
