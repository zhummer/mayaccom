<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Blogpost entity" internalId="7dfd07ad-44cb-46ac-9436-ee1df83576ac"><inputs name="blog_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_blogposts_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="blog"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Blog.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Blogpost related_blogposts = blogpostDAO.findBlogpostByPrimaryKey(related_blogposts_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;Blog blog = blogDAO.findBlogByPrimaryKey(blog_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;related_blogposts.setBlog(null);&#xD;&#xA;blog.getBlogposts().remove(related_blogposts);&#x9;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;blogpostDAO.remove(related_blogposts);&#xD;&#xA;&#x9;&#x9;blogpostDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#xD;&#xA;return blog;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
