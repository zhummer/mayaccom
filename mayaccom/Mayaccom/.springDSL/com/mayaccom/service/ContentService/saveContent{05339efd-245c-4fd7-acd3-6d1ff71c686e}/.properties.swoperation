<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Content entity" internalId="05339efd-245c-4fd7-acd3-6d1ff71c686e"><inputs name="content"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Content.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="Content existingContent = contentDAO.findContentByPrimaryKey(content.getId());&#xD;&#xA;&#xD;&#xA;if (existingContent != null)&#x9;{&#xD;&#xA;&#x9;if (existingContent != content) {&#xD;&#xA;&#x9;&#x9;&#x9;existingContent.setId (content.getId());&#xD;&#xA;&#x9;&#x9;&#x9;existingContent.setEditorContent (content.getEditorContent());&#xD;&#xA;&#x9;&#x9;&#x9;existingContent.setContentName (content.getContentName());&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;content = contentDAO.store(existingContent);&#xD;&#xA;}&#xD;&#xA;else&#x9;{&#xD;&#xA;&#x9;content = contentDAO.store(content);&#xD;&#xA;}&#xD;&#xA;contentDAO.flush();&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
