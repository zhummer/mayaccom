<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Host entity" internalId="70e8d936-1dbb-43e6-81ae-61502d814747"><inputs name="topic_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_host_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="topic"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Topic.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Topic topic = topicDAO.findTopicByPrimaryKey(topic_id, -1, -1);&#xD;&#xA;&#x9;&#x9;Host related_host = hostDAO.findHostByPrimaryKey(related_host_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;topic.setHost(null);&#xD;&#xA;related_host.getTopics().remove(topic);&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;topic = topicDAO.store(topic);&#xD;&#xA;&#x9;&#x9;topicDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;related_host = hostDAO.store(related_host);&#xD;&#xA;&#x9;&#x9;hostDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#x9;&#x9;hostDAO.remove(related_host);&#xD;&#xA;&#x9;&#x9;hostDAO.flush();&#xD;&#xA;&#xD;&#xA;return topic;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
