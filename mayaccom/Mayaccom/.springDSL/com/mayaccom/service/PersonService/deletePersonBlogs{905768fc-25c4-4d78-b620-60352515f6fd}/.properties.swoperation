<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Blog entity" internalId="905768fc-25c4-4d78-b620-60352515f6fd"><inputs name="person_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_blogs_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="person"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Person.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Blog related_blogs = blogDAO.findBlogByPrimaryKey(related_blogs_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;Person person = personDAO.findPersonByPrimaryKey(person_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;related_blogs.setPerson(null);&#xD;&#xA;person.getBlogs().remove(related_blogs);&#x9;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;blogDAO.remove(related_blogs);&#xD;&#xA;&#x9;&#x9;blogDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#xD;&#xA;return person;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
