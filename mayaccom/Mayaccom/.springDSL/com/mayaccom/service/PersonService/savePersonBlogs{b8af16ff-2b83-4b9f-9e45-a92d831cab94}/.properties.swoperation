<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Blog entity" internalId="b8af16ff-2b83-4b9f-9e45-a92d831cab94"><inputs name="id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_blogs"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Blog.datatype#/"/></dataType></inputs><outputs name="person"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Person.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="Person person = personDAO.findPersonByPrimaryKey(id, -1, -1);&#xD;&#xA;Blog existingblogs = blogDAO.findBlogByPrimaryKey(related_blogs.getId());&#xD;&#xA;&#xD;&#xA;&#xD;&#xA;// copy into the existing record to preserve existing relationships&#xD;&#xA;if (existingblogs != null)&#x9;{&#xD;&#xA;&#x9;&#x9;existingblogs.setId (&#xD;&#xA;&#x9;&#x9;related_blogs.getId());&#xD;&#xA;&#x9;&#x9;existingblogs.setTitle (&#xD;&#xA;&#x9;&#x9;related_blogs.getTitle());&#xD;&#xA;&#x9;&#x9;existingblogs.setDescription (&#xD;&#xA;&#x9;&#x9;related_blogs.getDescription());&#xD;&#xA;&#x9;related_blogs = existingblogs;&#xD;&#xA;}&#xD;&#xA;else&#x9;{&#xD;&#xA;&#x9;related_blogs = blogDAO.store(related_blogs);&#xD;&#xA;&#x9;blogDAO.flush();&#xD;&#xA;}&#xD;&#xA;&#xD;&#xA;related_blogs.setPerson(person);&#xD;&#xA;person.getBlogs().add(related_blogs);&#xD;&#xA;related_blogs = blogDAO.store(related_blogs);&#xD;&#xA;blogDAO.flush();&#xD;&#xA;&#xD;&#xA;person = personDAO.store(person);&#xD;&#xA;personDAO.flush();&#xD;&#xA;&#xD;&#xA;return person;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
