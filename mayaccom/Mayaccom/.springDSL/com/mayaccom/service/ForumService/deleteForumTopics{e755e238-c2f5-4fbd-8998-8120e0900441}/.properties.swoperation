<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Topic entity" internalId="e755e238-c2f5-4fbd-8998-8120e0900441"><inputs name="forum_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_topics_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="forum"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Forum.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Topic related_topics = topicDAO.findTopicByPrimaryKey(related_topics_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;Forum forum = forumDAO.findForumByPrimaryKey(forum_id, -1, -1);&#xD;&#xA;&#x9;&#xD;&#xA;related_topics.setForum(null);&#xD;&#xA;forum.getTopics().remove(related_topics);&#x9;&#x9;&#xD;&#xA;&#x9;&#xD;&#xA;&#x9;&#x9;topicDAO.remove(related_topics);&#xD;&#xA;&#x9;&#x9;topicDAO.flush();&#xD;&#xA;&#x9;&#x9;&#xD;&#xA;&#xD;&#xA;return forum;&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
