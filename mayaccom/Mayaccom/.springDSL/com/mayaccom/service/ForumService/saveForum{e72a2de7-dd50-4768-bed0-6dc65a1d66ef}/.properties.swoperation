<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Forum entity" internalId="e72a2de7-dd50-4768-bed0-6dc65a1d66ef"><inputs name="forum"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Forum.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="Forum existingForum = forumDAO.findForumByPrimaryKey(forum.getId());&#xD;&#xA;&#xD;&#xA;if (existingForum != null)&#x9;{&#xD;&#xA;&#x9;if (existingForum != forum) {&#xD;&#xA;&#x9;&#x9;&#x9;existingForum.setId (forum.getId());&#xD;&#xA;&#x9;&#x9;&#x9;existingForum.setName (forum.getName());&#xD;&#xA;&#x9;&#x9;&#x9;existingForum.setDescription (forum.getDescription());&#xD;&#xA;&#x9;}&#xD;&#xA;&#x9;forum = forumDAO.store(existingForum);&#xD;&#xA;}&#xD;&#xA;else&#x9;{&#xD;&#xA;&#x9;forum = forumDAO.store(forum);&#xD;&#xA;}&#xD;&#xA;forumDAO.flush();&#xD;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
